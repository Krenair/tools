#!/usr/bin/env python
from sr.tools.inventory.oldinv import *

gitdir = gettoplevel()
if not gitdir:
    # Not in the inventory, give up
    sys.exit(1)

templatedir = os.path.join(gitdir, ".meta", "assemblies")
templates = os.listdir(templatedir)

for template in templates:
    if template in ["default"]:
        continue

    print template
