#!/usr/bin/env python
from __future__ import print_function

import json
import urllib2

url = 'https://www.studentrobotics.org/ide/control.php/info/about'

page = urllib2.urlopen(url)
data = json.load(page)

if data is None:
	print "Failed to download from '%s'." % url
	exit(1)

#print data
print data['info']['Version']
